{
  "env": {
    "KV_REST_API_URL": "@kv_rest_api_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token"
  },
  "crons": [
    {
      "path": "/api/clear-messages",
      "schedule": "0 0 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/messages",
      "destination": "/api/messages"
    }
  ]
}